<ul class="nav-list u-plain-list">
    {{#foreach navigation}}
        <li
            class="menu-item menu-item-{{slug}}{{#if current}} menu-item-current{{/if}}">
            <a class="menu-item-link"
                href="{{url absolute="true"}}">{{label}}</a>
        </li>
    {{/foreach}}
    {{#if @labs.members}}
        <a class="subscribe-button" href="#subscribe">Subscribe</a>
    {{/if}}
</ul>
